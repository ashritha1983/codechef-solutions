import java.util.*;
import java.lang.*;
import java.io.*;

class main
{
	public static void main (String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		int T=sc.nextInt();
		while(T-->0){
		    int X=sc.nextInt();
		    int H=sc.nextInt();
		    if(X<H){
		        System.out.println("NO");
		    }else{
		            System.out.println("YES");
		    }
		    }
		    }
		}
	
